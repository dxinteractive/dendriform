{
    "extends": [
        "eslint-config-blueflag",
        "plugin:react/recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
    	"react/react-in-jsx-scope": "off"
    },
    "globals": {
        "React": "writable"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
